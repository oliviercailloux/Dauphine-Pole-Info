<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>CreatePerson</servlet-name>
    <servlet-class>PersonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreatePerson</servlet-name>
    <url-pattern>/createperson</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CreateCourse</servlet-name>
    <servlet-class>courseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateCourse</servlet-name>
    <url-pattern>/createcourse</url-pattern>
  </servlet-mapping>
</web-app>